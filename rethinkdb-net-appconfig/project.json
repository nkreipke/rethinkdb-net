{
  "version": "0.13.0",
  "authors": [ "Nico Kreipke" ],
  "description": "Application configuration loader for rethinkdb-net",

  "packOptions": {
    "owners": ["Mathieu Fenniak"],
    "releaseNotes": "See https://github.com/mfenniak/rethinkdb-net/blob/master/RELEASE-NOTES.md for detailed release notes.",
    "projectUrl": "https://github.com/mfenniak/rethinkdb-net",
    "licenseUrl": "https://raw.github.com/mfenniak/rethinkdb-net/master/LICENSE.txt",
    "requiresLicenseAcceptance": false,
    "files": {
      "includeFiles": ["../LICENSE.txt", "../README.md", "../RELEASE-NOTES.md"]
    }
  },

  "dependencies": {
    "AutoMapper": "5.1.1",
    "rethinkdb-net": { "target": "project" }
  },

  "buildOptions": {
    "outputName": "RethinkDb.AppConfig"
  },

  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.ComponentModel.DataAnnotations": "4.0.0.0",
        "System.Configuration": "4.0.0.0"
      }
    }
  }
}
